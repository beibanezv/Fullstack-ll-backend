# Backend .env.example
# Copy to .env and fill values before running

# Postgres connection string (used by pg). Example:
# DATABASE_URL=postgres://postgres:postgres@db:5432/perfumestore
DATABASE_URL=

# JWT secret for signing tokens
JWT_SECRET=change_this_secret

# Secret key used by legacy encrypt/decrypt helper
SECRET_KEY=change_this_secret_key

# Server port (default 5000)
PORT=5000

# bcrypt salt rounds
BCRYPT_SALT_ROUNDS=10
